{
    "version": 2,
    "name": "Hack-Academia-backend",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js"
    },
    {
        "rewrites": [
            {"source": "/(.*)", "destination": "/"}
        ]
    }
    ]
}